using System.Linq.Expressions;
using static System.Console;
string mes;
WriteLine("Digite o nome do mês:");
mes = Console.ReadLine();

    switch (mes)
{
	case "Janeiro":
		WriteLine("Esse mês tem 31 dias!");
		break;
	case "Fevereiro":
		WriteLine("Esse mês tem 28 dias!");
		break;
	case "Março":
		WriteLine("Esse mês tem 31 dias!");
		break;
	case "Abril":
		WriteLine("Esse mês tem 30 dias!");
		break;
	case "Maio":
		WriteLine("Esse mês tem 31 dias!");
		break;
	case "Junho":
		WriteLine("Esse mês tem 30 dias!");
		break;
	case "Julho":
		WriteLine("Esse mês tem 31 dias!");
		break;
	case "Agosto":
		WriteLine("Esse mês tem 31 dias!");
		break;
	case "Setembro":
		WriteLine("Esse mês tem 30 dias!");
		break;
	case "Outubro":
		WriteLine("Esse mês tem 31 dias!");
		break;
	case "Novembro":
		WriteLine("Esse mês tem 30 dias!");
		break;
	case "Dezembro":
		WriteLine("Esse mês tem 31 dias!");
		break;
	default:
		WriteLine($"Não existe mês com este número {mes}", Console.Out);
		break;
	}
ReadKey();
